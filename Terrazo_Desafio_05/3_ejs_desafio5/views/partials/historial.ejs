<div>
    <h2 style="color:crimson;">HISTORIAL PRODUCTOS</h2>
    <% if (DB_PRODUCTOS.length > 0) { %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: white;"> <th>Producto</th> <th>Precio</th> <th>Imagen</th> </tr>
            <% for (const producto of DB_PRODUCTOS) {%> 
            <tr>
                <td><%=producto.producto%>  </td>
                <td><%=producto.precio%> </td>
                <td>
                    <img width="30" src=<%=producto.imagen%> alt="">
                </td>
            </tr>
            <% } %>
        </table>
    </div>
    <% } else { %>
        <h3 class="alert alert-warning">No se encontraron productos</h3>
    <% } %>
</div>